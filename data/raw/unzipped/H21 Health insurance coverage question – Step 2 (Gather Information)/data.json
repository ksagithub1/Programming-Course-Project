                {
                    "keyword": "factors",
                    "score": 1,
                    "alternatives": []
                },
                {
                    "keyword": "medical necessity,",
                    "score": 1,
                    "alternatives": []
                },
                {
                    "keyword": "network providers,",
                    "score": 1,
                    "alternatives": []
                },
                {
