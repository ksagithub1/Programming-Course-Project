        {
            "externalId": "87661c20-b44d-4e61-ba90-a16898d1540b",
            "sequenceNumber": 15,
            "fileTranscript": "Yeah, that one's good to use. Can you put it for $40? Automated birthday gift?",
            "fileId": "b1ae3bcb-01c3-424c-af86-adc8aa359753",
            "actor": "SYM",
            "keywords": [],
            "settings": null
